<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkDialog" id="main_dialog">
    <property name="title">MemCARDuino communication</property>
    <property name="default-width">400</property>
    <property name="modal">True</property>
    <property name="deletable">False</property>
    <property name="resizable">false</property>
    <child internal-child="content_area">
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <property name="spacing">12</property>
        <property name="margin-top">12</property>
        <property name="margin-bottom">12</property>
        <property name="margin-start">12</property>
        <property name="margin-end">12</property>
        <child>
          <object class="GtkLabel" id="lbl_status">
            <property name="label"></property>
            <property name="xalign">0</property>
          </object>
        </child>
        <child>
          <object class="GtkProgressBar" id="progress_bar">
            <property name="show-text">False</property>
          </object>
        </child>
      </object>
    </child>
    <child internal-child="action_area">
      <object class="GtkBox">
        <property name="valign">center</property>
        <child>
          <object class="GtkButton" id="btn_abort">
            <property name="sensitive">False</property>
            <property name="label">Abort</property>
            <property name="width-request">80</property>
            <property name="margin-end">6</property>
            <property name="margin-bottom">6</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>